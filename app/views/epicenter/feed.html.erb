<div class="container"
<div class="row">
  <div class="col-xs-3" id="game-feed">
    <div class="game-feed"
      <center><h1>Games</h1></center>
      <div class="well">
        <h3><%= link_to "Rock Paper Scissors", rps_path %></h3>
      </div>
      <div class="well">
        <h3><%= link_to "Tic Tac Toe", ttt_path %></h3>
      </div>
    </div>
  </div>

  <div class="col-xs-3"></div>

  <div class="col-xs-9">
    <div class="post-feed">
      <center><h2>Posts <%= link_to 'New Post', new_post_path, remote: true, class: "btn btn-primary" %></h2></center>
      <div id="post-form" style="display:none;"></div>
        <ul>
          <div id="posts">
            <%= render @posts %>
          </div>
        </ul>
      </div>
    </div>
</div>
</div>
